using Arch.Xml;

/// <summary>
/// Server Info Class
/// </summary>
public class Server
{
    public Server()
    {
        
    }

    public Server(string name, string connectionString)
    {
        Name = name;
        ConnectionString = connectionString;
    }

    public string Name { get; set; }
    public string ConnectionString { get; set; }
}
